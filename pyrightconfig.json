{
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "venv"
    ],
    "ignore": [
        "src/oldstuff"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "stubPath": "stubs",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.10",
    "pythonPlatform": "Darwin",
    "extraPaths": [
        "."
    ]
} 